uniteste # ../uniteste.0

uniteste.descrever({"New Lisp Syntax" {
  uniteste.iguais({
    {
      dobro: x => x * 2
      ( dobro 5 )
    }[0]
    10
  })
  uniteste.iguais({
    {
      soma: args => args[0] + args[1]
      ( soma {3 7} )
    }[0]
    10
  })
  uniteste.iguais({
    {
      sem_args: _ => 42
      ( sem_args )
    }[0]
    42
  })
}})