uniteste # ../uniteste.0

uniteste.descrever({"Constantes (usando propriedades de lista)" {
  uniteste.iguais({
    {
      a: 5
      b: 8
      resultado: a + b
    }.resultado
    13
  })
  uniteste.iguais({
    {
      função1: _ => x == 42
      função2: _ => {
        x: 42
        função1(0)
      }[0]
      resultado: função2(0)
    }.resultado
    0
  })
}})