// Demonstração: vírgulas não são mais permitidas em listas

// ✅ FUNCIONAM - sintaxe com espaços
lista_valida1 = [1 2 3]
lista_valida2 = ["a" "b" "c"]
lista_valida3 = [
  nome: "João"
  "valor"
  idade: 30
]

// ✅ FUNCIONA - objetos ainda suportam vírgulas
objeto_com_virgulas = {
  nome: "João"
  idade: 30
  ativo: 1
}

