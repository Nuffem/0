lista = [
  mapear: args =>
    args[0][.] == 0 ? [] :
    args[0][.] == 1 ? [args[1](args[0][0])] :
    [args[1](args[0][0]) ...lista.mapear([args[0][1:] args[1]])]
  juntar: args =>
    args[0][.] == 0 ? "" :
    args[0][.] == 1 ? args[0][0] :
    `${args[0][0]}${args[1]}${lista.juntar([args[0][1:] args[1]])}`
]

lista